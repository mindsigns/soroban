<div class="col-md-6">
<h2 class="page-header">Add a new job</h2>

<%= render "form.html", changeset: @changeset,
                        services: @services,
                        jobtypes: @jobtypes,
                        clients: @clients,
                        today: @today,
                        action: job_path(@conn, :create) %>

  <div class="panel-footer">
  <%= link "Back", class: "btn btn-default", to: job_path(@conn, :index)%>
  </div>
</div>
</div>
</div>
