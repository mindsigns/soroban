<div class="col-md-6">
<h2 class="page-header">Edit job</h2>

<%= render "form.html", changeset: @changeset,
                        services: @services,
                        jobtypes: @jobtypes,
                        clients: @clients,
                        today: @today,
                        action: job_path(@conn, :update, @job) %>

<div class="panel-footer">
  <%= link "Back", to: job_path(@conn, :index), class: "btn btn-default" %>
</div>
</div>

